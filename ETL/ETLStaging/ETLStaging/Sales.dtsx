                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select  f.factid as Factid,
fd.idproduct as idproduct ,
f.customerid as customerid,
fd.lotid as lotid,
fd.idline as idline,
c.currencytype as currencytype,
c.currencynm as currencynm,
p.priceperunit as priceperunit,
fd.saleqty as saleqty,
f.discountamnt as discountamnt,
o.idorder as idorder,
f.DATE1 as date1
from fsales f
inner join invoicedet fd on f.factid= fd.invoice
inner join currency c on f.salecurrid= c.currencyid
inner join product p on fd.idproduct = p.productid
join dboorder o on f.factid = o.idorder;</property>
